{
  "name": "aframe-uipack-component",
  "version": "1.0.0",
  "description": "A experimental library for UI components for Aframe (media controls, buttons, menu, abstract controllers) ",
  "main": "index.js",
  "unpkg": "dist/aframe-uipack-component.min.js",
  "scripts": {
    "start": "webpack --mode development -w --devtool none index.js -o dist/aframe-uipack-component.js",
    "build": "webpack -p index.js -o dist/aframe-uipack-component.js && webpack -p index.js -o dist/aframe-uipack-component.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscarmarinmiro/aframe-uipack.git"
  },
  "keywords": [
    "aframe",
    "aframe-component",
    "aframe-vr",
    "vr",
    "webvr",
    "360",
    "stereo",
    "180",
    "video",
    "controls",
    "immersive-video"
  ],
  "author": "Oscar Marin <oscar@outliers.es>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oscarmarinmiro/aframe-uipack/issues"
  },
  "dependencies": {
    "howler": "^2.0.15"
  },
  "homepage": "https://github.com/oscarmarinmiro/aframe-uipack#readme",
  "devDependencies": {
    "aframe": "^0.9.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0"
  },
  "semistandard": {
    "globals": [
      "AFRAME",
      "THREE"
    ],
    "ignore": [
      "examples/build.js",
      "dist/**"
    ]
  }
}
